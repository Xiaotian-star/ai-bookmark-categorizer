{
  "manifest_version": 3,
  "name": "AI 书签分类助手",
  "version": "1.0",
  "description": "使用 AI 智能分类您的书签",
  "permissions": [
    "bookmarks",
    "storage",
    "webRequest"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*",
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
} 